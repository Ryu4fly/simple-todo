<div>
<% if notice.present? %>
  <%= notice %>
<% end %>
<%= form_tag do %>
  <%= label_tag :username, 'Username:' %>
  <%= text_field_tag :username, params[:username] %>

  <%= label_tag :password, 'Password:' %>
  <%= password_field_tag :password, params[:password] %>

  <%= submit_tag "Login" %>
<% end %>
</div>
